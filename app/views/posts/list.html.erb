<h1>Posts</h1>

<table>
  <thead>
    <tr>
    <th>ID</th>
    <th>Content</th>
    <th>User</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
  <tr>
    <td>
    <%= post.id %>
    </td>
    <td>
    <%= post.content %>
    </td>
    <td>
    <%= post.user_id%>
    </td>
  <% end %>
  </tbody>
</table>
